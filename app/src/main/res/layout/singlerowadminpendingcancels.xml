<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/adminpendingcardview"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="5dp"
    app:cardCornerRadius="10dp"
    app:cardUseCompatPadding="true">

    <RelativeLayout
        android:id="@+id/adminpendingrelative"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/card_view_border"
        android:padding="15dp">

        <TextView
            android:id="@+id/textView18"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="5dp"
            android:text="Bus Number" />

        <TextView
            android:id="@+id/Busnnotext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView18" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView18"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="4dp"
            android:text="Route" />

        <TextView
            android:id="@+id/routetext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Busnnotext"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView20" />

        <TextView
            android:id="@+id/Timetxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/routetext"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView25" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView20"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="5dp"
            android:text="Time" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView25"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="5dp"
            android:text="Air Condition" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Timetxt"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView29" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView29"
            android:layout_marginStart="15dp"
            android:layout_marginTop="5dp"
            android:text="Date" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView29"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView36"

            />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView36"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="5dp"
            android:text="Ticket Fee" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView37"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView31" />


        <TextView
            android:id="@+id/textView32"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView31"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="5dp"
            android:text="Total Ticket" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView14"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView32" />


        <TextView
            android:id="@+id/textView34"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView32"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="5dp"
            android:text="User Seats" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView33"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView34" />

        <TextView
            android:id="@+id/textView50"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView34"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="5dp"
            android:text="User No." />

        <TextView
            android:id="@+id/textView53"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView35"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView50" />

        <TextView
            android:id="@+id/textView51"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView50"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="5dp"
            android:text="Cancel Time" />

        <TextView
            android:id="@+id/textView52"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView53"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView51" />

        <Button
            android:id="@+id/button15"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView51"
            android:layout_alignParentStart="true"
            android:layout_marginTop="15dp"
            android:background="@drawable/buttonshape"
            android:text="Refund Pending!  Pay It" />

    </RelativeLayout>

</androidx.cardview.widget.CardView>